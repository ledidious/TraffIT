							<children>
								<Label fx:id="label01" text="Fahrzeuge" />
							</children>
						</Pane>
						<Pane layoutX="14.0" layoutY="234.0" prefHeight="49.0"
							prefWidth="93.0">
							<children>
								<ImageView fx:id="ivCar" fitHeight="55.0"
									fitWidth="93.0" preserveRatio="true">
									<image>
										<Image
											url="@../../../../../../../../../../../Pictures/TraffIT/car.png" />
									</image>
								</ImageView>
							</children>
						</Pane>
						<Pane layoutX="127.0" layoutY="234.0" prefHeight="49.0"
							prefWidth="146.0">
							<children>
								<ImageView fx:id="ivTruck" fitHeight="92.0"
									fitWidth="142.0" preserveRatio="true">
									<image>
										<Image
											url="@../../../../../../../../../../../Pictures/TraffIT/truck.png" />
									</image>
								</ImageView>
							</children>
						</Pane>
						<Pane layoutX="283.0" layoutY="234.0" prefHeight="49.0"
							prefWidth="93.0">
							<children>
								<ImageView fx:id="ivMotorcycle" fitHeight="49.0"
									fitWidth="109.0" layoutY="1.0" preserveRatio="true">
									<image>
										<Image url="@../../../../../resources/motorcycle.png" />
									</image>
								</ImageView>
							</children>
						</Pane>
						<Pane fx:id="iconPane01" layoutX="14.0" layoutY="51.0"
							prefHeight="79.0" prefWidth="82.0">
							<children>
								<ImageView fx:id="ivSpeedLimit50" fitHeight="92.0"
									fitWidth="82.0" preserveRatio="true" />
							</children>
						</Pane>
						<Pane layoutX="127.0" layoutY="51.0" prefHeight="79.0"
							prefWidth="82.0">
							<children>
								<ImageView fx:id="ivSpeedLimit70" fitHeight="79.0"
									fitWidth="82.0" preserveRatio="true" />
							</children>
						</Pane>
						<Pane layoutX="241.0" layoutY="51.0" prefHeight="79.0"
							prefWidth="82.0">
							<children>
								<ImageView fx:id="ivSpeedLimit100" fitHeight="79.0"
									fitWidth="82.0" />
							</children>
						</Pane>
						<Pane layoutX="360.0" layoutY="51.0" prefHeight="79.0"
							prefWidth="93.0">
							<children>
								<ImageView fx:id="ivConstruction" fitHeight="79.0"
									fitWidth="93.0" onMouseDragReleased="#handleDeletePerson"
									preserveRatio="true" />
							</children>
						</Pane>
						<Pane layoutX="830.0" layoutY="357.0" prefHeight="31.0"
							prefWidth="125.0" AnchorPane.bottomAnchor="8.0"
							AnchorPane.rightAnchor="323.0">
							<children>
								<Button fx:id="button03" layoutX="1.0"
									mnemonicParsing="false" onAction="#handleDeletePerson"
									text="Stop Simulation" />
							</children>
						</Pane>
						<Pane layoutX="677.0" layoutY="357.0" prefHeight="31.0"
							prefWidth="125.0" AnchorPane.bottomAnchor="8.0"
							AnchorPane.rightAnchor="476.0">
							<children>
								<Button fx:id="button04" layoutX="1.0"
									mnemonicParsing="false" onAction="#handleDeletePerson"
									text="Start Simulation" />
							</children>
						</Pane>
						<Pane fx:id="targetPane" layoutX="534.0" layoutY="51.0"
							prefHeight="218.0" prefWidth="395.0"
							style="-fx-background-color: red;">
							<children>
								<ImageView fx:id="ivTarget" fitHeight="56.0"
									fitWidth="142.0" layoutX="239.0" layoutY="148.0"
									pickOnBounds="true" preserveRatio="true" />
							</children>
						</Pane>
					</children>
				</AnchorPane>
			</items>
		</SplitPane>
	</children>
</AnchorPane>
