	fx:controller="de.superdudes.traffit.application.GUIController">
	<children>
		<!--set SplitPane values "0.0" to fit/stretch the child elements inside 
			the anchorpane -->
		<SplitPane id="splitPane" dividerPositions="0.5"
			minHeight="800.0" minWidth="1280.0" orientation="VERTICAL"
			prefHeight="800.0" prefWidth="1280.0" AnchorPane.bottomAnchor="0.0"
			AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
			AnchorPane.topAnchor="0.0">
			<items>
				<AnchorPane minHeight="0.0" minWidth="0.0"
					prefHeight="100.0" prefWidth="160.0">
					<children>
						<GridPane maxHeight="Infinity" maxWidth="Infinity"
							prefHeight="395.0" prefWidth="1278.0"
							AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
							AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"
							style="-fx-background-color: darkseagreen;">
							<columnConstraints>
								<ColumnConstraints hgrow="ALWAYS"
									minWidth="10.0" prefWidth="100.0" />
							</columnConstraints>
							<rowConstraints>
								<RowConstraints minHeight="10.0"
									prefHeight="30.0" vgrow="ALWAYS" />
								<RowConstraints minHeight="10.0"
									prefHeight="30.0" vgrow="ALWAYS" />
								<RowConstraints minHeight="10.0"
									prefHeight="30.0" vgrow="ALWAYS" />
							</rowConstraints>
							<children>
								<Pane id="lane1" fx:id="lane1" prefHeight="200.0"
									prefWidth="200.0" style="-fx-background-color: darkseagreen;"
									GridPane.rowIndex="1" />
								<Pane fx:id="lane2" prefHeight="200.0" prefWidth="200.0"
									style="-fx-background-color: darkseagreen;"
									GridPane.rowIndex="2" />
								<Pane fx:id="signlane" prefHeight="200.0" prefWidth="200.0" />
							</children>
						</GridPane>
					</children>
				</AnchorPane>
				<AnchorPane minHeight="0.0" minWidth="0.0"
					prefHeight="100.0" prefWidth="160.0">
					<children>
						<Pane layoutX="986.0" layoutY="356.0" prefHeight="31.0"
							prefWidth="125.0" AnchorPane.bottomAnchor="8.0"
							AnchorPane.rightAnchor="167.0">
							<children>
								<Button fx:id="button01" mnemonicParsing="false"
									onAction="#handleDeletePerson" text="Load Simulation" />
							</children>
						</Pane>
						<Pane layoutX="1138.0" layoutY="356.0" prefHeight="31.0"
							prefWidth="127.0" AnchorPane.bottomAnchor="8.0"
							AnchorPane.rightAnchor="13.0">
							<children>
								<Button fx:id="button02" mnemonicParsing="false"
									onAction="#changeTextFromLabel" text="Save Simulation" />
							</children>
						</Pane>
						<Pane layoutX="14.0" layoutY="22.0" prefHeight="17.0"
							prefWidth="93.0" AnchorPane.leftAnchor="14.0"
							AnchorPane.topAnchor="22.0">
							<children>
								<Label text="Verkehrsschilder" />
							</children>
						</Pane>
						<Pane layoutX="14.0" layoutY="204.0" prefHeight="17.0"
